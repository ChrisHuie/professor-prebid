{
	"name": "Prebid Tools V0.2 (experimental)",
	"version": "2.10",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"run_at": "document_start",
			"js": [
				"js/inject.js",
				"js/content.js",
				"js/jquery-3.5.1.min.js"
			]
		}
	],
	"web_accessible_resources": [
		"js/injected.js"
	],
	"browser_action": {
		"default_tile": "Message",
		"default_popup": "popup.html"
	},
	"permissions": [
		"webRequest",
		"storage",
		"tabs",
		"*://*/"
	]
}