{
	"name": "Prebid Tools V0.2 (experimental)",
	"version": "2.10",
	"manifest_version": 2,
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"run_at": "document_end",
			"js": [
				"js/3p/moment.min.js",
				"js/3p/dataframe.js",
				"js/inject.js",
				"js/content.js"
			],
			"css" : [ "resources/content-styles.css"]
		}
	],
	"web_accessible_resources": [
		"js/3p/dataframe.js",
		"js/3p/moment.min.js",
		"js/injected.js",
		"resources/content-styles.css"
	],
	"browser_action": {
		"default_tile": "Message",
		"default_popup": "popup.html"
	},
	"permissions": [
		"webRequest",
		"storage",
		"tabs",
		"*://*/"
	]
}